{
  "name": "webpack-4",
  "version": "1.0.0",
  "description": "Folder WebPack Course",
  "main": "index.js",
  "scripts": {
    "build:manual": "wbepack --entry ./index.js --output ./bundle.js --mode development",
    "build": "webpack",
    "build:external": "webpack --config ./external-entry/webpack.config.js",
    "build:multi:entry": "webpack --config ./multi-entry/webpack.config.js",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js",
    "build:plugins": "webpack --config ./plugins/webpack.config.js",
    "build:dev": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js",
    "build:babel": "webpack --config ./babel/webpack.config.js"
  },
  "keywords": [
    "webpack"
  ],
  "author": "Camilo Araque Caro",
  "license": "ISC",
  "dependencies": {
    "webpack": "4.44.0",
    "webpack-cli": "3.3.12"
  },
  "devDependencies": {
    "@babel/core": "7.11.1",
    "@babel/preset-env": "7.11.0",
    "babel-loader": "8.1.0",
    "css-loader": "4.2.0",
    "html-webpack-plugin": "4.3.0",
    "mini-css-extract-plugin": "0.9.0",
    "style-loader": "1.2.1",
    "webpack-dev-server": "3.11.0"
  }
}
