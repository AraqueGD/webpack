{
  "name": "webpack-4",
  "version": "1.0.0",
  "description": "Folder WebPack Course",
  "main": "index.js",
  "scripts": {
    "build:manual": "wbepack --entry ./index.js --output ./bundle.js --mode development",
    "build": "webpack",
    "build:external": "webpack --config ./external-entry/webpack.config.js",
    "build:multi:entry": "webpack --config ./multi-entry/webpack.config.js",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js",
    "build:plugins": "webpack --config ./plugins/webpack.config.js",
    "build:dev": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js",
    "build:babel": "webpack --config ./babel/webpack.config.js",
    "build:react": "webpack-dev-server --config ./react/webpack.config.js",
    "build:url": "webpack-dev-server --config ./url-loader/webpack.config.js",
    "build:sass": "webpack-dev-server --config ./styles-pre/webpack.config.js",
    "build:prevent": "webpack --config ./prevent-dul/webpack.config.js",
    "build:dll": "webpack --config ./dll/webpack.dll.config.js",
    "build:prevent-fast": "webpack --config ./dll/webpack.config.js",
    "build:dynamic-dll": "webpack --config ./dynamic-imports/webpack.dll.config.js",
    "build:dynamic": "webpack --config ./dynamic-imports/webpack.config.js"
  },
  "keywords": [
    "webpack"
  ],
  "author": "Camilo Araque Caro",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "7.11.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "sass": "^1.26.10",
    "webpack": "4.44.0",
    "webpack-cli": "3.3.12"
  },
  "devDependencies": {
    "@babel/core": "7.11.1",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.11.0",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-react": "7.10.4",
    "babel-loader": "8.1.0",
    "css-loader": "4.2.0",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "4.3.0",
    "mini-css-extract-plugin": "0.9.0",
    "sass-loader": "9.0.3",
    "style-loader": "1.2.1",
    "url-loader": "4.1.0",
    "webpack-dev-server": "3.11.0"
  }
}
